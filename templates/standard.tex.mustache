% Change tags for mustache {{=<< >>=}}
% LaTeX resume using res.cls
% Downloaded from http://www.rpi.edu/dept/arc/training/latex/resumes/
\documentclass[line,margin]{res}
%\usepackage{helvetica} % uses helvetica postscript font (download helvetica.sty)
%\usepackage{newcent}   % uses new century schoolbook postscript font

\usepackage{hyperref}

%% Defined here so that the arguments can be reordered
\newenvironment{project}[4]
{{\textbf{#1}} \hfill #4\\
  \textsc{#2}  \hfill #3\vspace{0.5em}
  \begin{itemize} \itemsep -1pt}
  {\end{itemize}}

\begin{document}

\name{<<name>>}
% \address used up to three times to have up to three lines of address
\address{
  <<#contact>>
  <<#address>><<street>>, <<city>>, <<state>> <<zip>><</address>> \\
  <<phone>> - <<email>> \\
  <<#websites>>
  <<url>> \\
  <</websites>>
  <</contact>>
}

\begin{resume}
  <<#skills>>
  \section{SKILLS \\ LANGUAGES}
  <<#meta.length>>
  \textbf{Skills} \hfill
  \begin{minipage}[t]{0.8\textwidth}
    <<#meta>>
    <<#line>><<entry>><<^last>>, <</last>><</line>> \\
    <</meta>>
  \end{minipage}
  <</meta.length>>
  <<#languages.length>>
  \textbf{Languages} \hfill
  \begin{minipage}[t]{0.8\textwidth}
    <<#languages>>
    <<#line>><<entry>><<^last>>, <</last>><</line>> \\
    <</languages>>
  \end{minipage}
  <</languages.length>>
  <<#frameworks.length>>
  \textbf{Frameworks} \hfill
  \begin{minipage}[t]{0.8\textwidth}
    <<#frameworks>>
    <<#line>><<entry>><<^last>>, <</last>><</line>>

    <</frameworks>>
  \end{minipage}
  <</frameworks.length>>

  <<#projects.length>>
  \section{PROJECTS}
  <</projects.length>>
  <<#projects>>
  \textbf{<<title>>} \hfill
  <<#websites>><<#homepage>><<short>><</homepage>><</websites>>

  \textsc{<<language>>}

  \begin{itemize} \itemsep -1pt
    <<#bulletpoints>>\item <<.>><</bulletpoints>>
  \end{itemize}
  <</projects>>
  <</skills>>

  <<#jobs.length>>
  \section{WORK \\ EXPERIENCE}
  <</jobs.length>>
  <<#jobs>>
  \begin{minipage}{\textwidth}
    \textbf{<<title>>} \hfill <<#dates>><<from>> - <<to>><</dates>>

    \textsc{<<company>>} \hfill
    <<#location>><<city>>, <<state>><</location>>

    \small{<<#languages>><<.>> <</languages>>}
    \vspace{0.5em}
    \begin{itemize} \itemsep -1pt
      <<#accomplishments>>
    \item
      <<summary>>
      <<#details>>
      \begin{itemize}
      \item <<.>>
      \end{itemize}
      <</details>>
      <</accomplishments>>
    \end{itemize}
  \end{minipage}

  <</jobs>>

  <<#education.length>>
  \section{EDUCATION}
  <</education.length>>
  <<#education>>
  \begin{minipage}{\textwidth}
    \textbf{<<title>>} \hfill <<date>>\\
    <<#source>>
    \textsc{<<name>>} \hfill <<city>>, <<state>>\\
    <</source>>

    <<#courses.length>>
    {\bf Relevant Coursework}\\
    <</courses.length>>
    <<#courses>>
    <<#line>><<entry>><<^last>>, <</last>><</line>> \\
    <</courses>>
    <<#projects.length>>

    {\bf Projects}
    <</projects.length>>
    \begin{itemize}
      <<#projects>>
    \item <<label>> \\ <<url>>
      <</projects>>
    \end{itemize}
  \end{minipage}
  <</education>>
\end{resume}
\end{document}
